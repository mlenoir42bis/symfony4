{% extends 'base.html.twig' %}

    {% block body %}
        
<div class="section no-pad-bot" id="index-banner">
    <div class="container">
      
      <div id="container-list-assemble">

        <div id="list-assemble">

                <h3 style="text-align: center;">{{ assembles.title }}</h3>
                <hr>
                
                <h4>Htag</h4>
                {% for htag in assembles.htag %}

                  <div class="btn blue lighten-3">
                    {{ htag.name }}
                  </div>

                {% endfor %}

                <h4>Sujet de l'assemblé</h4>
                <div class="z-depth-1">
                <p>{{ assembles.subject }}</p>
                </div>
                
                <h4>Resume de l'assemblé</h4>
                <div class="z-depth-1">
                <p>{{ assembles.resume }}</p>
                </div>
                
                <h4>Analyse préliminaire de l'assemblé</h4>
                <div class="z-depth-1">
                <p>{{ assembles.analyse }}</p>
                </div>
                

          
        </div>
      </div>

    </div>
  </div>
</div>


    {% endblock %}

